import java.util.*;
public class Day4 {

    public static void main(String[] args) {

        Scanner sc=new Scanner(System.in);
        String s=sc.next(), p=sc.next();
    
        int count=0, index=s.indexOf(p);
    
        while(index!=-1){
            count++;
            index=s.indexOf(p,index+1);
        }
      System.out.print(count);
  }
}
