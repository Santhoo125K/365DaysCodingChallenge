public class Day107{
  public static void main (String[]args){

    int[] arr = { 1, 1, 1 };

    int X = 1;
    int Y = 1;
    int ans = 0;

    for (int i = 0; i < arr.length; i++){
      arr[i]--;
    }
    ans = ans + X;

    for (int i = 0; i < arr.length; i++){
	    if (arr[i] != 0){
	      arr[i] = 0;
	      ans = ans + Y;
	    }
    }
    
    System.out.println (ans);
  
  }
}
